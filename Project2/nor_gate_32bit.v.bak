module nor_gate_32bit(
    input [31:0] a,
    input [31:0] b,
    output [31:0] out
);
    genvar i;
    generate
        for (i = 0; i < 32; i = i + 1) begin : nor_gate_array
            nor_gate_1bit nor_gate_inst(
                .a(a[i]),
                .b(b[i]),
                .out(out[i])
            );
        end
    endgenerate
endmodule
